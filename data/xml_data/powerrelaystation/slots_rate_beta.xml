<?xml version="1.0"?>
<root type = "slotsrate">

	<!--CPhysical-->

	<!--nazwa obiektu-->
	<genre genre = "Slots rate beta" />
	
	<!--zakres dla promienia kolizji-->
	<radius
		circle_radius = "45.254834"
	/>
	
	<!--rozmiar obszaru prostokatnego-->
	<!--width-->
	<!--height-->	
	<!--body - width-->
	<!--body - height-->
	<!--head - width-->
	<!--head - height-->	
	<rect_size
		rect_size_x = "64.0"
		rect_size_y = "64.0"	
		rect_size_body_x = "64.0"
		rect_size_body_y = "64.0"
		rect_size_head_x = "64.0"
		rect_size_head_y = "64.0"		
	/>
	
	<!--skala-->
	<scale
		scale_body = "1.0"
		scale_head = "1.0"
	/>

	<!--pulap obiektu wzgledem podloza-->
	<altitude
		altitude = "1.0"
	/>

	<!--flagi decydujace o wygladzie obiektu-->
	<physical_config
		use_displayable_body  = "true"
		use_shadow_body       = "true"
		use_displayable_head  = "false"
		use_shadow_head       = "false"
	/>
	
	<!--flagi decydujace o kolidowaniu poszczegolnych skladowych-->
	<!--bool flaga kolidowania - body (default true)-->
	<!--bool flaga kolidowania - body shadow-->
	<!--bool flaga kolidowania - head-->
	<!--bool flaga kolidowania - head shadow-->
	<colliding_config
		colliding_body			= "true"
		colliding_body_shadow	= "false"
		colliding_head			= "false"
		colliding_head_shadow	= "false"
	/>	
	
	<!--CActor-->
	
	<!--dane techniczne-->
	<technical_data
		mass        = "5"
		speed       = "0"
		movabled    = "false"
	/>

	<!--zestawy animacji-->
	<!--nazwa animacji musi byc zgodna z nazwami zarejestrowqanymi, class CAnimationManager-->
	<!--typ animacji musi byc zgodny z wpisami w tablicy zawierajcej zdefiniowane typy animacji, class CAnimSet-->
	<animset name_animset = "animset_slots_rate_one">
		<!--body-->
		<anim name = "anim_slots_rate_beta_body_0"		type = "slots_rate_body_0"	/>
		<anim name = "anim_slots_rate_beta_body_1"		type = "slots_rate_body_1"	/>
		<anim name = "anim_slots_rate_beta_body_2"		type = "slots_rate_body_2"	/>
		<anim name = "anim_slots_rate_beta_body_3"		type = "slots_rate_body_3"	/>
		<anim name = "anim_slots_rate_beta_body_4"		type = "slots_rate_body_4"	/>
		<!--head-->
		<!--obiekt nie obs³uguje animacji head-->
	</animset>

	<animset name_animset = "animset_slots_rate_two">
		<!--body-->
		<anim name = "anim_slots_rate_beta_body_0"		type = "slots_rate_body_0"	/>
		<anim name = "anim_slots_rate_beta_body_1"		type = "slots_rate_body_1"	/>
		<anim name = "anim_slots_rate_beta_body_2"		type = "slots_rate_body_2"	/>
		<anim name = "anim_slots_rate_beta_body_3"		type = "slots_rate_body_3"	/>
		<anim name = "anim_slots_rate_beta_body_4"		type = "slots_rate_body_4"	/>
		<!--head-->
		<!--obiekt nie obs³uguje animacji head-->
	</animset>

</root>
