<?xml version="1.0"?>
<root type = "battery">

	<!--CPhysical-->

	<!--nazwa obiektu-->
	<genre genre = "Battery Beta" />

	<!--zakres dla promienia kolizji-->
	<radius
		circle_radius = "45.25"
	/>
	
	<!--rozmiar obszaru prostokatnego-->
	<!--width-->
	<!--height-->
